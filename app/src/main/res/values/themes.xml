        <item name="shapeAppearance">@style/MyShapeAppearanceRightRounded</item>
    </style>

    <style name="MyShapeAppearanceRightRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">10dp</item>
        <item name="cornerSizeBottomRight">10dp</item>
        <item name="cornerSizeTopLeft">0dp</item>
        <item name="cornerSizeBottomLeft">0dp</item>
    </style>

    <style name="MyShapeAppearanceLeftRounded">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSizeTopRight">0dp</item>
        <item name="cornerSizeBottomRight">0dp</item>
        <item name="cornerSizeTopLeft">10dp</item>
        <item name="cornerSizeBottomLeft">10dp</item>
    </style>

    <style name="LoginTextInputLayoutStyle" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.Dense">
        <item name="boxStrokeColor">@color/teal_200</item>
    </style>

    <style name="Theme.MyRetail.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.MyRetail.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorControlNormal">@color/black</item>
    </style>

    <style name="Theme.MyRetail.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark">

    </style>


